			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'patch'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=patch"
				],
				"filter": {
					"name": "patch",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
